extends header

block MainView
  
  ul(class="content")
    if children.length == 0
      li child이 없습니다. 첫 child을 만들어 주세요!
    if userid === undefined
      each child in children
        li
          a(href="/set/"+child._id)
            h2= child.title
    else
      each child in children
        li
          a(href="/userset/"+child._id+"/"+userid)
            h2= child.title
