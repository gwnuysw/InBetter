extends header

block MainView
  ul(class="horizen-menu-left")
    if uppersettitle == "LinkAbout"
      li
        a(href="/") Upper set
    else if uppersettitle === undefined
      li
        a(href="/") Upper set
    else
      li
        a(href="/set/"+uppersetid+"/"+uppersettitle) Upper Set
    li
      a(href="/link/"+cursetid+"/"+cursettitle) Links
    li
      a(href="#") Communities
    li
      a(href="/set/newsetform/"+cursetid+"/"+cursettitle) New Set
    li
      a(href="/link/newlinkform/"+cursetid+"/"+cursettitle) New link
    li
      a(href="#") New Community
  ul(class="content")
    if children.length == 0
      li child이 없습니다. 첫 child을 만들어 주세요!
    each child in children
      li
        a(href="/set/"+child._id+"/"+child.title)
          h2= child.title
